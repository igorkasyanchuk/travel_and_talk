- f.inputs do
  = f.input :email, :autocorrect => 'off', :autocapitalize => 'off'
  = f.input :first_name
  = f.input :last_name
  = f.input :password
  = f.input :password_confirmation
  = f.input :admin, :hint => '(only for development)', :label => 'Register as Admin'

- f.buttons do
  = f.submit "Sign up"